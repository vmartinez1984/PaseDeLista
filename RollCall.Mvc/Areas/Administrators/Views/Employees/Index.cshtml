@model RollCall.Dto.ListEmployeeDto

@{
    ViewData["Title"] = "Index";
}

<h1>Lista de empleados</h1>
@{await Html.RenderPartialAsync("_FormSearchEmployee",new RollCall.Dto.SearchEmployeeDto());}
<p>
    <a asp-action="Create">Agregar nuevo</a>
</p>
<div class="row">
    @foreach(var item in Model.ListEmployee)
    {
       await Html.RenderPartialAsync("_Card", item);
    }
</div>
@{await Html.RenderPartialAsync("_Pager",Model);}